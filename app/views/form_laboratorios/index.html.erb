<h1>Laboratorios</h1>

<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Paciente</th>
      <th>Usuario</th>
      <th>Hospital</th>
      <th>Fecha Toma de Muestra</th>
      <th>Fecha de Entrega</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @form_laboratorios.each do |form_laboratorio| %>
      <tr>
        <td><%= form_laboratorio.nombre %></td>
        <td><%= form_laboratorio.paciente %></td>
        <td><%= form_laboratorio.usuario %></td>
        <td><%= form_laboratorio.hospital %></td>
        <td><%= form_laboratorio.fecha_toma_muestra %></td>
        <td><%= form_laboratorio.fecha_entrega %></td>
        <td><%= link_to 'Ver', form_laboratorio %></td>
        <td><%= link_to 'Editar', edit_form_laboratorio_path(form_laboratorio) %></td>
        <td><%= link_to 'Eliminar', form_laboratorio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Formulario Laboratorio', new_form_laboratorio_path, :class=> "btn" %>

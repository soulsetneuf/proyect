<h1>Tuberculosis</h1>

<table class="table table-striped table-hover" id="tabla">
  <thead>
    <tr>
      <th>Paciente</th>
      <th>Hospital</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @form_tuberculoses.each do |form_tuberculose| %>
      <tr>
        <td><%= form_tuberculose.paciente.nombres %></td>
        <td><%= form_tuberculose.hospital %></td>
        <td><%= link_to 'Ver', form_tuberculose %></td>
        <td><%= link_to 'Editar', edit_form_tuberculose_path(form_tuberculose) %></td>
        <td><%= link_to 'Eliminar', form_tuberculose, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td>
          <%= link_to '<i class="glyphicon glyphicon-list-alt"></i>'.html_safe, {:format => "pdf", :controller => "form_tuberculoses", action: "show", :id => form_tuberculose}, :"data-toggle" => "tooltip", :"data-placement" => "bottom", :title => "Crear  Informe", :target => "_blank" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo  Formulario Tuberculosis', new_form_tuberculose_path, :class=> "btn" %>

<div id="chart-container"></div>
<script type="text/javascript">
  console.log(gon.query);
</script>
<script type="text/javascript">
  FusionCharts.ready(grafico_columna("chart-container","Historial de registro del ultimo a√±o Tuberculosis","Meses","Cantidad de pacientes registrados",gon.reporte_mensual_tuberculoses));
</script>
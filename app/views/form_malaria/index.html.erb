<h1>Malaria</h1>

<table class="table table-striped table-hover" id="tabla">
  <thead>
    <tr>
      <th>Paciente</th>
      <th>Hospital</th>
      <th>Antecedentes: Ha tenido malaria y/o harecibido tratamiento antimalarico en el mes</th>
      <th>Diagnostico</th>
      <th>Tratamiento</th>
      <th>Completo tratamiento</th>
      <th>Lugar donde fue tratado</th>
      <th>Lugar probable de infeccion. donde se encontraba hace dos semanas</th>
      <th>Ocupacion</th>
      <th>Tipo de caso</th>
      <th>Inicio de sintomas inicio de la fiebre</th>
      <th>Tipo de examen</th>
      <th>tipo de Prueba</th>
      <th>Resultado control</th>
      <th>Densidad parasitaria</th>
      <th>Peso</th>
      <th>Tratamiento supervisado</th>
      <th>Requiere trat. malaria grave</th>
      <th>Persisten sintomas</th>
      <th>Fecha de control</th>
      <th>Firma</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @form_malaria.each do |form_malarium| %>
      <tr>
        <td><%= form_malarium.paciente.nombres %></td>
        <td><%= form_malarium.hospital %></td>
        <td><%= form_malarium.tiene_malar_trat_antimalarico %></td>
        <td><%= form_malarium.diagnostico %></td>
        <td><%= form_malarium.tratami %></td>
        <td><%= form_malarium.compl_trat %></td>
        <td><%= form_malarium.lugar_tratado %></td>
        <td><%= form_malarium.lug_hace_sema %></td>
        <td><%= form_malarium.ocupacion %></td>
        <td><%= form_malarium.tipo_caso %></td>
        <td><%= form_malarium.inicio_sint %></td>
        <td><%= form_malarium.tipo %></td>
        <td><%= form_malarium.prueba %></td>
        <td><%= form_malarium.resultado %></td>
        <td><%= form_malarium.densidad_paras %></td>
        <td><%= form_malarium.peso %></td>
        <td><%= form_malarium.trat_superv %></td>
        <td><%= form_malarium.req_trat_mal_grave %></td>
        <td><%= form_malarium.persis_sintom %></td>
        <td><%= form_malarium.fecha_control %></td>
        <td><%= form_malarium.firma %></td>
        <td><%= link_to 'Ver', form_malarium %></td>
        <td><%= link_to 'Editar', edit_form_malarium_path(form_malarium) %></td>
        <td><%= link_to 'Eliminar', form_malarium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td>
          <%= link_to '<i class="glyphicon glyphicon-list-alt"></i>'.html_safe, {:format => "pdf", :controller => "form_malaria", action: "show", :id => form_malarium}, :"data-toggle" => "tooltip", :"data-placement" => "bottom", :title => "Crear  Informe", :target => "_blank" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Formulario Mamalaria', new_form_malarium_path, :class=> "btn" %>


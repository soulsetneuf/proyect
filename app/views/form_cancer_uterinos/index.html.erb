        <td><%= link_to 'Editar', edit_form_cancer_uterino_path(form_cancer_uterino) %></td>
        <td><%= link_to 'Eliminar', form_cancer_uterino, method: :delete, data: {confirm: 'Are you sure?'} %></td>
        <td>
          <%= link_to '<i class="glyphicon glyphicon-list-alt"></i>'.html_safe, {:format => "pdf", :controller => "form_cancer_uterinos", action: "show", :id => form_cancer_uterino}, :"data-toggle" => "tooltip", :"data-placement" => "bottom", :title => "Crear  Informe", :target => "_blank" %>
        </td>
      </tr>
  <% end %>
